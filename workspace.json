{
    "version": 2,
    "cli": { "defaultCollection": "@nrwl/react" },
    "defaultProject": "frontend",
    "generators": {
        "@nrwl/react": { "application": { "babel": true } },
        "@nrwl/next": { "application": { "style": "styled-jsx", "linter": "eslint" } }
    },
    "projects": {
        "frontend": {
            "root": "examples/frontend",
            "sourceRoot": "examples/frontend",
            "projectType": "application",
            "targets": {
                "build": {
                    "executor": "@nrwl/next:build",
                    "outputs": ["{options.outputPath}"],
                    "options": { "root": "examples/frontend", "outputPath": "dist/examples/frontend" },
                    "configurations": { "production": {} }
                },
                "serve": {
                    "executor": "@nrwl/next:server",
                    "options": { "buildTarget": "frontend:build", "dev": true },
                    "configurations": { "production": { "buildTarget": "frontend:build:production", "dev": false } }
                },
                "export": { "executor": "@nrwl/next:export", "options": { "buildTarget": "frontend:build:production" } },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/examples/frontend"],
                    "options": { "jestConfig": "examples/frontend/jest.config.js", "passWithNoTests": true }
                },
                "lint": { "executor": "@nrwl/linter:eslint", "options": { "lintFilePatterns": ["examples/frontend/**/*.{ts,tsx,js,jsx}"] } }
            }
        },
        "frontend-e2e": {
            "root": "examples/frontend-e2e",
            "sourceRoot": "examples/frontend-e2e/src",
            "projectType": "application",
            "targets": {
                "e2e": {
                    "executor": "@nrwl/cypress:cypress",
                    "options": {
                        "cypressConfig": "examples/frontend-e2e/cypress.json",
                        "tsConfig": "examples/frontend-e2e/tsconfig.e2e.json",
                        "devServerTarget": "frontend:serve"
                    },
                    "configurations": { "production": { "devServerTarget": "frontend:serve:production" } }
                },
                "lint": { "executor": "@nrwl/linter:eslint", "options": { "lintFilePatterns": ["examples/frontend-e2e/**/*.{js,ts}"] } }
            }
        },
        "qapp-qapp-core": {
            "root": "packages/qapp/qapp-core",
            "sourceRoot": "packages/qapp/qapp-core/src",
            "projectType": "library",
            "targets": {
                "build": {
                    "executor": "@nrwl/web:package",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "outputPath": "dist/packages/qapp/qapp-core",
                        "tsConfig": "packages/qapp/qapp-core/tsconfig.lib.json",
                        "project": "packages/qapp/qapp-core/package.json",
                        "entryFile": "packages/qapp/qapp-core/src/index.ts",
                        "external": ["redux-saga/effects"],
                        "babelConfig": "@nrwl/react/plugins/bundle-babel",
                        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
                        "assets": [{ "glob": "README.md", "input": ".", "output": "." }],
                        "buildableProjectDepsInPackageJsonType": "dependencies"
                    }
                },
                "lint": { "executor": "@nrwl/linter:eslint", "options": { "lintFilePatterns": ["packages/qapp/qapp-core/**/*.{ts,tsx,js,jsx}"] } },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/packages/qapp/qapp-core"],
                    "options": { "jestConfig": "packages/qapp/qapp-core/jest.config.js", "passWithNoTests": true }
                }
            }
        },
        "qapp-qapp-react": {
            "root": "packages/qapp/qapp-react",
            "sourceRoot": "packages/qapp/qapp-react/src",
            "projectType": "library",
            "targets": {
                "build": {
                    "executor": "@nrwl/web:package",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "outputPath": "dist/packages/qapp/qapp-react",
                        "tsConfig": "packages/qapp/qapp-react/tsconfig.lib.json",
                        "project": "packages/qapp/qapp-react/package.json",
                        "entryFile": "packages/qapp/qapp-react/src/index.ts",
                        "external": [],
                        "babelConfig": "@nrwl/react/plugins/bundle-babel",
                        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
                        "assets": [{ "glob": "README.md", "input": ".", "output": "." }],
                        "buildableProjectDepsInPackageJsonType": "dependencies"
                    }
                },
                "lint": { "executor": "@nrwl/linter:eslint", "options": { "lintFilePatterns": ["packages/qapp/qapp-react/**/*.{ts,tsx,js,jsx}"] } },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/packages/qapp/qapp-react"],
                    "options": { "jestConfig": "packages/qapp/qapp-react/jest.config.js", "passWithNoTests": true }
                }
            }
        }
    }
}
