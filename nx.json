{
    "implicitDependencies": {
        "workspace.json": "*",
        "package.json": { "dependencies": "*", "devDependencies": "*" },
        "tsconfig.base.json": "*",
        "tslint.json": "*",
        ".eslintrc.json": "*",
        "nx.json": "*"
    },
    "affected": { "defaultBase": "master" },
    "npmScope": "toolbox",
    "tasksRunnerOptions": {
        "default": { "runner": "@nrwl/workspace/tasks-runners/default", "options": { "cacheableOperations": ["build", "lint", "test", "e2e"] } }
    },
    "workspaceLayout": { "appsDir": "examples", "libsDir": "packages" },
    "projects": {
        "reduxify": { "tags": [] },
        "core": { "tags": [] },
        "frontend": { "tags": [] },
        "frontend-e2e": { "tags": [], "implicitDependencies": ["frontend"] },
        "qapp-qapp-core": { "tags": [] }
    }
}
